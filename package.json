{
  "name": "ma-backend",
  "version": "0.8.0",
  "description": "backend for the phishing awareness app",
  "main": "src/server.ts",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "build": "tsc",
    "dev": "npm run build && npm start",
