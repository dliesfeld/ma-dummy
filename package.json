    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express"
  ],
  "author": "Dominik Liesfeld ich@dominik-liesfeld.de",
  "license": "ISC",
  "dependencies": {
    "@types/cheerio": "^0.22.35",
    "body-parser": "^1.20.2",
    "cheerio": "^1.0.0-rc.12",
    "connect-mongodb-session": "^2.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "googleapis": "^140.0.1",
    "mail-listener5": "^2.1.2",
    "mongoose": "^8.4.0",
    "pino": "^8.20.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "socket.io": "^4.7.5"
